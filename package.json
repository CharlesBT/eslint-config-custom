{
  "name": "@charlesbt/eslint-config-custom",
  "version": "3.6.0",
  "description": "Custom eslint config",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlesBT/eslint-config-custom.git"
  },
  "author": {
    "name": "Charles Br√©bant",
    "email": "charles.brebant@gmail.com",
    "url": "https://github.com/CharlesBT"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepack": "unbuild",
    "lint": "unbuild && eslint --fix .",
    "format": "prettier --write .",
    "tc": "tsc --noEmit",
    "clean": "rimraf -g ./node_modules ./pnpm-lock.yaml ./.pnpm-debug.log",
    "reinstall": "pnpm clean && pnpm install",
    "pkg:outdated": "pnpm -r outdated"
  },
  "dependencies": {
    "@eslint/js": "^9.30.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^10.2.0",
    "eslint-plugin-vuetify": "^2.5.2",
    "globals": "^16.2.0",
    "typescript-eslint": "^8.35.0"
  },
  "devDependencies": {
    "@types/node": "22",
    "eslint": "^9.30.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}